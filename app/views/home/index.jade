extends ../shared/template

block content
  h1= title
  input#user(type='text')
  button#join join gameses
  input#count(type='text')
  button#pick How many letters, fo?
  
  table#users
    thead
      tr
        th User
        th Letters
        th Nickname
    tbody

  table#board
    tbody
      - for(var i = 0; i<99 ; i++)
        tr
          - for(var j = 0; j<99 ; j++)
            td(data-x=j,data-y=i)